{
    "class_diagram": {
        "classes": [
            {
                "name": "$class_name",
                "attributes": [
                    {
                        "name": "$attribute_name",
                        "type": "$attribute_type"
                    }
                ],
                "methods": [
                    {
                        "name": "$method_name",
                        "type": "$method_type"
                    }
                ],
                "position": {
                    "x": "$x",
                    "y": "$y"
                }
            }
        ],
        "connections" : [
            {
                "source": "$class_id    //referencing .json file classes",
                "target": "$class_id    //referencing .json file classes",
                "source_arrow": "$arrow_number //0,1,2 - type of arrow end",
                "target_arrow": "$arrow_number //0,1,2 - type of arrow end"
            }
        ]
    },

    "seq_diagram": {
        "objects": [
            {
                "name": "$object_name",
                "class": "$class_id     //referencing .json file classes",
                "position": {
                    "x": "$x",
                    "y": "$y"
                },
                "anchors": "$anchors_number"
            }
        ],
        "actors": [
            {
                "name": "$actor_name",
                "position": {
                    "x": "$x",
                    "y": "$y"
                },
                "anchors": "$anchors_number"
            }
        ],
        "messages": [
            {
                "target_class": "$class_id     //referencing .json file classes",
                "id_method": "method_id    //referencing .json file objects",
                "source": {
                    "id": "$object_id   //referencing .json file objects",
                    "position": "$num   //-1 left, -2 right"
                },
                "target": {
                    "id": "$object_id   //referencing .json file objects",
                    "position": "$num   //-1 left, -2 right"
                }
            }
        ],
        "activations": [
            {
                "item": "$object_id   //referencing .json file objects",
                "source_position": "$num",
                "target_position": "$num"
            }
        ]
    }
}